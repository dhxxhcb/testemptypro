<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.xoa.dao.users.UserFunctionMapper" >
  <resultMap id="userResultMap" type="com.xoa.model.users.UserFunction" >
    <id column="uid" property="uid" jdbcType="INTEGER" />
    <result column="user_id" property="userId" jdbcType="VARCHAR" />
    <result column="user_func_id_str" property="userFunCidStr" jdbcType="VARCHAR" />
  </resultMap>
  
  
   <select id="getDatagrid" resultMap="userResultMap">
		SELECT uid,user_id,user_func_id_str from user_function
	</select>
</mapper>